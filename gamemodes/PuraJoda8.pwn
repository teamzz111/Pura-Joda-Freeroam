	stock IsValidName(string[])
	{
	    for(new i = 0, j = strlen(string); i < j; i++)
	    {
	        switch(string[i])
	        {
	            case '0' .. '9': continue;
	            case 'a' .. 'z': continue;
	            case 'A' .. 'Z': continue;
	            case '_': continue;
	            case '$': continue;
	            case '.': continue;
	            case '=': continue;
	            case '(': continue;
	            case ')': continue;
	            case '[': continue;
	            case ']': continue;
	            default: return 0;
	        }
	    }
	    return 1;
	}


