dcmd_aceptar(playerid, params[])
{
	if(params[0] == '\0' || !IsNumeric(params))
	    return SendUsage(playerid, "/aceptar [playerid]");

	if(g_DuelInProgress == 1)
		return SendError(playerid, "Otro duelo está en curso en el momento, espera a que el duelo haya terminado!");

    new
		DuelID = strvalEx(params),
		pName[MAX_PLAYER_NAME],
		zName[MAX_PLAYER_NAME],
		tString[128];

    if(DuelID != g_GotInvitedToDuel[playerid])
    	return SendError(playerid, "Ese Jugador no te ha invitado a un Duelo!");

	GetPlayerName(playerid, pName, MAX_PLAYER_NAME);
	GetPlayerName(DuelID, zName, MAX_PLAYER_NAME);

 	format(tString, sizeof(tString), "Has aceptado duelo con %s [ID:%d], El duelo empezara en 10 segundos..",zName,DuelID);
 	SendClientMessage(playerid, COLOR_YELLOW, tString);

 	format(tString, sizeof(tString), "%s [ID:%d], aceptó el duelo con usted, el duelo comenzará en 10 segundos..",pName,playerid);
 	SendClientMessage(DuelID, COLOR_YELLOW, tString);

 	format(tString, sizeof(tString), "[Pura Joda] Duelo entre %s y %s se iniciará en 10 segundos",pName,zName);
 	SendClientMessageToAll(COLOR_ORANGE, tString);
 	SendClientMessageToAll(COLOR_ORANGE,"Utiliza /VerDesafio o /VipDesafio Para Ver el Desafio ---> Estos Cobran");
 	InitializeDuel(playerid);
 	InitializeDuelEx( DuelID);

 	g_IsPlayerDueling[playerid] = 1;
 	g_IsPlayerDueling[DuelID] = 1;

  	g_DuelingID1 = playerid;
    g_DuelingID2 = DuelID;

	g_DuelInProgress = 1;

	return 1;
}


dcmd_desafio(playerid, params[])
{
	if(params[0] == '\0' || !IsNumeric(params))
	    return SendUsage(playerid, "/desafio [playerid]");

	if(g_HasInvitedToDuel[playerid] == 1)
		return SendError(playerid, "Ya invitó a alguien a un duelo! (Tipo, /Rdesafio para restablecer su invitación)");

	new
		DuelID = strvalEx(params),
		pName[MAX_PLAYER_NAME],
		zName[MAX_PLAYER_NAME],
		tString[128];

	GetPlayerName(playerid, pName, MAX_PLAYER_NAME);
	GetPlayerName(DuelID, zName, MAX_PLAYER_NAME);

	if (!IsPlayerConnected(DuelID))
	    return SendError(playerid, "Jugador no conectado.");

 	if(	g_HasInvitedToDuel[DuelID] == 1)
 		return SendError(playerid, "El Jugador ya tiene invitación para duelo!");

	if(	DuelID  == playerid)
 		return SendError(playerid, "No puedes realizar duelos contigo mismo estúpido!");

 	format(tString, sizeof(tString), "Has invitado a %s [ID:%d] a un duelo de 1 en 1, Espera a qe %s Acepte la Invitacion.",zName, DuelID, zName);
 	SendClientMessage(playerid, COLOR_YELLOW, tString);

 	format(tString, sizeof(tString), "Tienes invitacion de %s [ID:%d] a un duelo de 1 en 1, Usa /aceptar [id] para aceptar y comenzar el duelo-MARICA-.",pName, playerid);
 	SendClientMessage(DuelID, COLOR_YELLOW, tString);

 	g_GotInvitedToDuel[DuelID] = playerid;
	g_HasInvitedToDuel[playerid] = 1;

	return 1;
}
