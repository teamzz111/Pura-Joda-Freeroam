	public OnPlayerCommandPerformed(playerid, cmdtext[], success)
	{
		if(success)
	{
		return 1;
	}
	if(GetPVarInt(playerid, "MINION") == 1) return 0;
	new tmp[256];
	new playermoney;
	new sendername[MAX_PLAYER_NAME];
	new giveplayer[MAX_PLAYER_NAME];
	new cmd[256];
	new giveplayerid, moneys, idx;
    new  string[128];
	cmd = strtok(cmdtext, idx);
//===========================================================================================================//
	if(!strcmp(cmdtext, "/rampa"))
	{
	    if(rampab[playerid] == false)
	   {
    		SendClientMessage(playerid, LIGHTGREEN, "[Pura Joda] Acabas de activar las rampas, para desactivarlas vuelve a ingresar el comando.");
			rampab[playerid] = true;
       }
       else if(rampab[playerid] == true)
       {
            SendClientMessage(playerid, LIGHTGREEN, "[Pura Joda] Acabas de desactivar las rampas, para activar vuelve a ingresar el comando.");
			rampab[playerid] = false;
	   }

	return 1;
	}

    if(!strcmp(cmdtext, "/netstats"))
    {
        new stats[400+1];
        GetNetworkStats(stats, sizeof(stats)); // get the servers networkstats
        ShowPlayerDialog(playerid, DIALOG_OTROS, DIALOG_STYLE_MSGBOX, "Server Network Stats", stats, "Close", "");
        return 1;
    }
	if (!strcmp("/laseron", cmdtext, true)) {
		SetPVarInt(playerid, "laser", 1);
		SetPVarInt(playerid, "color", GetPVarInt(playerid, "color"));
		return 1;
	}

	if (!strcmp("/laseroff", cmdtext, true)) {
		SetPVarInt(playerid, "laser", 0);
		RemovePlayerAttachedObject(playerid, 0);
		return 1;
	}

	if (!strcmp("/lasercol", cmd, true)) {
		tmp = strtok(cmdtext, idx);
		if (!strlen(tmp)) {
			SendClientMessage(playerid, 0x00E800FF, "Usa: /lasercol [color]");
			return 1;
		}
		if (!strcmp(tmp, "rojo", true)) SetPVarInt(playerid, "color", 18643);
		else if (!strcmp(tmp, "azul", true)) SetPVarInt(playerid, "color", 19080);
		else if (!strcmp(tmp, "rosado", true)) SetPVarInt(playerid, "color", 19081);
		else if (!strcmp(tmp, "naranja", true)) SetPVarInt(playerid, "color", 19082);
		else if (!strcmp(tmp, "verde", true)) SetPVarInt(playerid, "color", 19083);
		else if (!strcmp(tmp, "amarillo", true)) SetPVarInt(playerid, "color", 19084);
		else SendClientMessage(playerid, 0x00E800FF, "Color no disponible!");
		return 1;
	}
	if(!strcmp(cmd, "/fr", true) || !strcmp(cmd, "/rf",true))
	{
		if(GetPlayerMoney(playerid) < 500)
        {
            SendClientMessage(playerid,COLOR_RED,"Necesitas $500");
		}
		else if(!IsPlayerInAnyVehicle(playerid) || (GetPlayerState(playerid) != PLAYER_STATE_DRIVER)) return SendClientMessage(playerid,COLOR_RED,"Necesitas un auto para usar éste comando-");
		new VehicleID, Float:Angle, Float:X, Float:Y, Float:Z;
		GetPlayerPos(playerid, X, Y, Z);
