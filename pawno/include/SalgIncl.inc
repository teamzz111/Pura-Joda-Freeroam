stock GetNumberOfPlayersOnThisIP(test_ip[])
{
	new against_ip[32+1];
	new x = 0;
	new ip_count = 0;
	for(x=0; x<MAX_PLAYERS; x++)
	{
		if(IsPlayerConnected(x))
		{
		    GetPlayerIp(x,against_ip,32);
		    if(!strcmp(against_ip,test_ip)) ip_count++;
		}
	}
	return ip_count;
}

stock RangeBan(playerid)
{
	new pos, oldpos, temporal[128], ip[15], ip2[15];

	GetPlayerIp(playerid, ip, sizeof(ip));
	pos = strfind(ip, ".", true);
	pos++;

	for(new i = 0; i < pos; i++)
	{
		ip2[i] = ip[pos-pos+i];
	}
	pos--;
	ip[pos] = ' ';
	oldpos = pos;
	oldpos++;
	pos = strfind(ip, ".", true);
	pos++;

	for(new i = oldpos; i < pos; i++)
	{
		ip2[i] = ip[pos-pos+i];
	}

	format(ip2, sizeof(ip2), "%s*.*", ip2);
	format(temporal, sizeof(temporal), "banip %s", ip2);
	SendRconCommand(temporal);
	SaveToRBan(ip2);
	Kick(playerid);
	return ip2;
}